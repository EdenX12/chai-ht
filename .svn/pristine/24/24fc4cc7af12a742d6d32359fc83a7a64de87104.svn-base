    <select id="listUser" resultType="com.tian.sakura.cdd.db.domain.user.SUser">
        SELECT
        a.*, b.level_name AS userLevelTypeName,
        IFNULL(c.taskAmount, 0) AS taskAmount,
        IFNULL(c.taskNum, 0) AS taskNum,
        IFNULL(c.taskNumSum, 0) AS taskNumSum,
        IFNULL(d.orderAmount, 0) AS orderAmount,
        IFNULL(d.orderNum, 0) AS orderNum
        FROM
        s_user a
        LEFT JOIN s_user_level b ON a.user_level_type = b.level_type
        LEFT JOIN (
            SELECT
            SUM(pay_amount) AS taskAmount,
            count(*) AS taskNum,
            sum(task_number) AS taskNumSum,
            user_id
            FROM
            `s_user_task`
            WHERE
            pay_status = '1'
            GROUP BY
            user_id
        ) c ON a.id = c.user_id
        LEFT JOIN (
            SELECT
            SUM(pay_amount) AS orderAmount,
            user_id,
            COUNT(*) AS orderNum
            FROM
            s_order_detail
            WHERE
            order_status IN ('1', '2', '3')
            GROUP BY
            user_id
        ) d ON a.id = d.user_id
        WHERE
        1 = 1
        <if test="nickName != null and nickName != ''">
            and a.nick_name like concat('%', #{nickName}, '%')
        </if>
        <if test="userStatus != null">
            and a.user_status = #{userStatus}
        </if>
        <if test="userLevelType != null">
            and a.user_level_type = #{userLevelType}
        </if>
        <if test="userType != null">
            and a.user_type = #{userType}
        </if>
        order by a.create_time desc
    </select>

