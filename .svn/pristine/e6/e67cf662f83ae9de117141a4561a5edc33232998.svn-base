package com.tian.sakura.cdd.db.domain.auth.vo;


import com.tian.sakura.cdd.db.domain.auth.TConResource;

import java.util.List;

/**
 * 挂载资源vo
 *
 * @author lvzonggang
 * @Date 2019/4/10
 */
public class MountResourceVo {

    private String permissionId;
    private String menuId;
    private List<TConResource> resourceList;

    public String getPermissionId() {
        return permissionId;
    }

    public void setPermissionId(String permissionId) {
        this.permissionId = permissionId;
    }

    public String getMenuId() {
        return menuId;
    }

    public void setMenuId(String menuId) {
        this.menuId = menuId;
    }

    public List<TConResource> getResourceList() {
        return resourceList;
    }

    public void setResourceList(List<TConResource> resourceList) {
        this.resourceList = resourceList;
    }

}
